    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "influxdb-client": "^1.32.0",
    "nodemailer": "^6.9.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.4",
